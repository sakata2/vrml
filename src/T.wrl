# T Logo
DEF T Transform {
	children [
		Transform { 
			translation 0 0 -1
			children [
				Shape {
					appearance Appearance {
						material Material {
							diffuseColor 0 0 1
						}
					}
					geometry IndexedFaceSet {
						solid TRUE
						coord Coordinate{
							point	[
							-40	40	3,
							-40	30	3,
							40	30	3,
							40	40	3,
							-6	30	3,
							-6	-40	3,
							6	-40	3,
							6	30	3
							]
						}
						coordIndex	[
							0,1,2,3,-1,
							4,5,6,7,-1
						]
					}
				}
			]
		}
	]
}


